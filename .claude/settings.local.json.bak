      {
        "matcher": "Read|Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "prompt",
            "prompt": "BLOQUEADO: Use Serena MCP!\n\nTool bloqueada. Alternativas:\n- Ler: mcp__plugin_serena_serena__read_file\n- Criar: mcp__plugin_serena_serena__create_text_file\n- Editar: mcp__plugin_serena_serena__replace_content"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "plugin-dev@claude-plugins-official": true,
    "hookify@claude-plugins-official": true,
    "systemic-agent-orchestrator": true
  }
}
